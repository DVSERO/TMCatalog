<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TMCatalog.View.Resources"
                    xmlns:userControls="clr-namespace:TMCatalog.View"
                    xmlns:tmCatalogContentTypes="clr-namespace:TMCatalog.Common.TMCatalogContentTypes;assembly=TMCatalog.Common">

  <DataTemplate x:Key="VehicleSearchTemplate" DataType="{x:Type tmCatalogContentTypes:IVehicleSearchContent}">
    <userControls:VehicleSelectionUserControl />
  </DataTemplate>

  <DataTemplate x:Key="VehicleTypeTemplate" DataType="{x:Type tmCatalogContentTypes:IVehicleTypeContent}">
    <userControls:VehicleTypeUserControl />
  </DataTemplate>

  <DataTemplate x:Key="ShopingBasketTemplate" DataType="{x:Type tmCatalogContentTypes:IVehicleTypeContent}">
    <userControls:ShoppingBasket />
  </DataTemplate>

</ResourceDictionary>